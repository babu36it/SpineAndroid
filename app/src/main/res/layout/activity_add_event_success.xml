<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.home.menus.podcasts.addrss.AddRssActivity">

        <ImageButton
            android:contentDescription="@string/app_name"
            android:id="@+id/imageButton36"
            style="@style/BackButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_close_24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView191"
            style="@style/OnlyText"
            android:textSize="@dimen/_14sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/thank_you"
            android:textAllCaps="true"
            app:layout_constraintBottom_toBottomOf="@+id/imageButton36"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageButton36" />

        <View
            android:id="@+id/view70"
            style="@style/ViewActionLine"
            android:layout_width="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView191" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/eight"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view70">


            <ImageView
                android:contentDescription="@string/app_name"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal"
                android:layout_margin="@dimen/_16sdp"
                android:padding="@dimen/_4sdp"
                app:backgroundTint="@color/colorPrimaryDark"
                android:background="@drawable/boarder_round_btn_bg"
                android:src="@drawable/ic_check" />


            <TextView
                android:id="@+id/textView279"
                style="@style/NormalText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:text="@string/your_event_is_live_on_the_spiritual_nnetwork_app"
                android:textAlignment="center"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="parent" />


            <Button
                android:id="@+id/button97"
                app:backgroundTint="@color/colorPrimaryDark"
                style="@style/ButtonRound"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_15sdp"
                android:text="@string/go_to_event" />


            <Button
                android:id="@+id/button98"
                app:backgroundTint="@color/colorPrimaryDark"
                style="@style/ButtonRound"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_15sdp"
                android:text="@string/add_another_event" />


        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>